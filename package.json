{
  "name": "blog-redux",
  "version": "1.0.0",
  "description": "Let's begin with the blog template in `public` folder. (Based on an awesome repo here https://github.com/BlackrockDigital/startbootstrap-clean-blog)",
  "main": "index.js",
  "scripts": {
    "dev:play": "npm run start:play",
      "start:play": "node ./step_play/frontweb/server/hotServer.js",

    "dev:0": "npm run start:0",
      "start:0": "node ./step_0/frontweb/server/hotServer.js",

    "dev:1": "npm run start:1 & npm run server:dev:1",
      "start:1": "node ./step_1/frontweb/server/hotServer.js",
      "server:dev:1": "node ./step_1/frontweb/bin/server.js",

    "dev:2": "npm run start:2 & npm run server:dev:2",
      "start:2": "node ./step_2/frontweb/server/hotServer.js",
    "prod:2": "npm run server:prod:2",
      "preprod:2": "npm run build:2",
      "prebuild:2": "rimraf ./step_2/frontweb/public/dist/*",
      "build:2": "webpack --config ./step_2/frontweb/webpack/build.config.js -d --progress",
      "server:dev:2": "node ./step_2/frontweb/bin/server.js",
      "server:prod:2": "NODE_ENV=production node ./step_2/frontweb/bin/server.js",

    "dev:3": "npm run start:3 & npm run server:dev:3",
      "start:3": "node ./step_3/frontweb/server/hotServer.js",
    "prod:3": "npm run server:prod:3",
      "preprod:3": "npm run build:3",
      "prebuild:3": "rimraf ./step_3/frontweb/public/dist/*",
      "build:3": "webpack --config ./step_3/frontweb/webpack/build.config.js -d --progress",
      "server:dev:3": "node ./step_3/frontweb/bin/server.js",
      "server:prod:3": "NODE_ENV=production node ./step_3/frontweb/bin/server.js",
    "devadmin:3": "npm run startadmin:3 & npm run server:devadmin:3",
      "startadmin:3": "node ./step_3/admin/server/hotServer.js",
    "prodadmin:3": "npm run server:prodadmin:3",
      "preprodadmin:3": "npm run buildadmin:3",
      "prebuildadmin:3": "rimraf ./step_3/admin/public/dist/*",
      "buildadmin:3": "webpack --config ./step_3/admin/webpack/build.config.js -d --progress",
      "server:devadmin:3": "node ./step_3/admin/bin/server.js",
      "server:prodadmin:3": "NODE_ENV=production node ./step_3/admin/bin/server.js",

    "dev:final": "npm run start:final & npm run server:dev:final",
      "start:final": "node ./step_final/frontweb/server/hotServer.js",
    "prod:final": "npm run server:prod:final",
      "preprod:final": "npm run build:final",
      "prebuild:final": "rimraf ./step_final/frontweb/public/dist/*",
      "build:final": "webpack --config ./step_final/frontweb/webpack/build.config.js -d --progress",
      "server:dev:final": "node ./step_final/frontweb/bin/server.js",
      "server:prod:final": "NODE_ENV=production node ./step_final/frontweb/bin/server.js",
    "devadmin:final": "npm run startadmin:final & npm run server:devadmin:final",
      "startadmin:final": "node ./step_final/admin/server/hotServer.js",
    "prodadmin:final": "npm run server:prodadmin:final",
      "preprodadmin:final": "npm run buildadmin:final",
      "prebuildadmin:final": "rimraf ./step_final/admin/public/dist/*",
      "buildadmin:final": "webpack --config ./step_final/admin/webpack/build.config.js -d --progress",
      "server:devadmin:final": "node ./step_final/admin/bin/server.js",
      "server:prodadmin:final": "NODE_ENV=production node ./step_final/admin/bin/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jepezi/blog-redux.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jepezi/blog-redux/issues"
  },
  "homepage": "https://github.com/jepezi/blog-redux#readme",
  "dependencies": {
    "babel-plugin-transform-react-constant-elements": "6.5.0",
    "babel-plugin-transform-react-inline-elements": "6.6.5",
    "babel-plugin-transform-runtime": "6.6.0",
    "babel-polyfill": "6.7.2",
    "babel-register": "6.7.2",
    "body-parser": "1.15.0",
    "classnames": "2.2.3",
    "express-session": "1.13.0",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.6.1",
    "normalizr": "2.0.0",
    "react": "15.0.0-rc.2",
    "react-dom": "15.0.0-rc.2",
    "react-markdown": "2.1.0",
    "react-redux": "4.4.1",
    "react-router": "2.0.1",
    "redux": "3.3.1",
    "serialize-javascript": "1.2.0"
  },
  "devDependencies": {
    "autoprefixer": "6.3.3",
    "babel-core": "6.7.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-es2015-native-modules": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.5.0",
    "css-loader": "0.23.1",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "json-loader": "0.5.4",
    "node-sass": "3.4.2",
    "postcss-loader": "0.8.2",
    "redux-logger": "2.6.1",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "stats-webpack-plugin": "0.3.1",
    "style-loader": "0.13.0",
    "webpack": "2.1.0-beta.4",
    "webpack-dev-middleware": "1.5.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
